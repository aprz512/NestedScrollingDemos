        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aprz.nestedscrolldemos.nestedscroll.wxsportrank.SportRankActivity" />
        <activity android:name=".custombehavior.custom1.CustomBehaviorActivity1" />
        <activity android:name=".custombehavior.custom2.CustomBehaviorActivity2" />
        <activity android:name=".custombehavior.custom3.CustomBehaviorActivity3" android:theme="@style/NoTitleAppTheme" />
        <activity android:name=".drag.demo01.DragActivity1" />
        <activity android:name=".drag.demo02.YoutubeActivity" />
    </application>

</manifest>